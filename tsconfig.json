{
  "extends": "./.quasar/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "lib": ["es2019", "dom", "webworker"],
    "types": ["node"],
    "paths": {
      "#q-app/*": [".quasar/*"],
      "@/*": ["src/*"],
      "src/*": ["src/*"],
      "app/*": [".quasar/*"],
      "@assets/*": ["src/assets/*"],
      "components/*": ["src/components/*"],
      "layouts/*": ["src/layouts/*"],
      "pages/*": ["src/pages/*"],
      "assets/*": ["src/assets/*"],
      "boot/*": ["src/boot/*"]
    }
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src-pwa/**/*.ts",
    "src/**/*.vue",
    "./auto-imports.d.ts",
    "./components.d.ts",
    ".eslintrc.cjs",
    "quasar.config.ts"
  ]
}
